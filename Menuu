local missionCoords = Vector3.new(-1071.29, 10.10, -1299.06)
local npcCoords = {
    Vector3.new(-1024.22, 10.10, -1349.36),
    Vector3.new(-1051.34, 10.10, -1433.55),
    Vector3.new(-981.36, 10.10, -1463.35),
    Vector3.new(-859.90, 10.10, -1450.89),
    Vector3.new(-854.08, 10.10, -1379.46),
    Vector3.new(-928.99, 10.10, -1310.65),
    Vector3.new(-971.01, 10.10, -1289.84),
    Vector3.new(-950.25, 10.10, -1377.54)
}

local running = false
local speed = 75 -- Velocidade do elevador (aumentado para ser mais r치pido)

local function elevatorMove(startPos, endPos)
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
    
    if humanoidRootPart then
        local distance = (endPos - startPos).Magnitude
        local duration = distance / speed
        local elapsedTime = 0

        while elapsedTime < duration do
            if not running then break end
            local alpha = elapsedTime / duration
            local newPos = startPos:Lerp(endPos, alpha) -- Interpola entre startPos e endPos
            humanoidRootPart.CFrame = CFrame.new(newPos)
            elapsedTime = elapsedTime + game:GetService("RunService").Heartbeat:Wait()
        end

        if running then
            humanoidRootPart.CFrame = CFrame.new(endPos) 
        end
    end
end

local function missionNpcInteraction()
    print("Interagindo com o NPC da miss칚o...")
    wait(7) 
end

local function npcInteraction(coords)
    print("Interagindo com o NPC em: " .. tostring(coords))
    wait(1) 
end

local function elevatorSystem()
    while running do
        elevatorMove(game.Players.LocalPlayer.Character.HumanoidRootPart.Position, missionCoords)
        if not running then break end
        missionNpcInteraction()
       
        for _, coords in ipairs(npcCoords) do
            if not running then break end
            elevatorMove(game.Players.LocalPlayer.Character.HumanoidRootPart.Position, coords)
            if not running then break end
            npcInteraction(coords)
        end
        
        if not running then break end
        elevatorMove(game.Players.LocalPlayer.Character.HumanoidRootPart.Position, missionCoords)
    end
end

local function startElevator()
    if not running then
        running = true
        spawn(elevatorSystem) -- Inicia o processo de movimenta칞칚o
    end
end

local function stopElevator()
    running = false
end

local player = game.Players.LocalPlayer
local combatItem

-- Fun칞칚o para equipar o item 'Combat'
local function equiparCombate()
    if player then
        local backpack = player.Backpack
        combatItem = backpack:FindFirstChild("Combat")
        if combatItem then
            combatItem.Parent = player.Character
        else
            warn("Item 'Combate' n칚o encontrado na mochila.")
        end
    else
        warn("Jogador n칚o encontrado.")
    end
end

-- Fun칞칚o para desequipar o item 'Combat'
local function desequiparCombate()
    if player and combatItem and player.Character then
        combatItem.Parent = player.Backpack
    end
end

-- Fun칞칚o que realiza o ciclo de equipar e desequipar a cada 0,3 segundos
local function cicloEquiparDesequipar()
    while running do
        equiparCombate()
        wait(0.3)
        desequiparCombate()
        wait(0.3)
    end
end

TabFarms:AddToggle({
    Name = "Auto Farm NPCS LEVEL 30 (V1)",
    Default = false,
    Callback = function(state)
        running = state
        if running then
            -- Inicia o sistema de elevador em uma nova thread
            spawn(elevatorSystem)
            spawn(cicloEquiparDesequipar)
        else
            stopElevator() -- Para o sistema de elevador
        end
    end
})

local missionCoords = Vector3.new(-655.21, 24.53, -1187.81)
local npcCoords = {
    Vector3.new(-747.96, 24.13, -1220.91),
    Vector3.new(-718.34, 24.13, -1233.69),
    Vector3.new(-721.92, 24.13, -1264.63),
    Vector3.new(-699.76, 24.13, -1293.81),
    Vector3.new(-702.82, 24.13, -1333.12)
}

local running = false
local speed = 75 -- Velocidade do elevador

local function elevatorMove(startPos, endPos)
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
    
    if humanoidRootPart then
        local distance = (endPos - startPos).Magnitude
        local duration = distance / speed
        local elapsedTime = 0

        while elapsedTime < duration do
            if not running then return end 
            local alpha = elapsedTime / duration
            local newPos = startPos:Lerp(endPos, alpha)
            humanoidRootPart.CFrame = CFrame.new(newPos)
            elapsedTime = elapsedTime + game:GetService("RunService").Heartbeat:Wait()
        end

        humanoidRootPart.CFrame = CFrame.new(endPos)
    end
end

local function missionNpcInteraction()
    print("Interagindo com o NPC da miss칚o...")
    wait(7)
end

local function npcInteraction(coords)
    print("Interagindo com o NPC em: " .. tostring(coords))
    wait(1)
end

local function elevatorSystem()
    while running do
        elevatorMove(game.Players.LocalPlayer.Character.HumanoidRootPart.Position, missionCoords)
        if not running then break end 
        missionNpcInteraction()
        if not running then break end 
        
        for _, coords in ipairs(npcCoords) do
            elevatorMove(game.Players.LocalPlayer.Character.HumanoidRootPart.Position, coords)
            if not running then break end -- Verifica se o sistema ainda est치 ativo
            npcInteraction(coords)
            if not running then break end -- Verifica se o sistema ainda est치 ativo
        end
        
        elevatorMove(game.Players.LocalPlayer.Character.HumanoidRootPart.Position, missionCoords)
    end
end

