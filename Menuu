        end
    end
})

-- Vari치vel para controlar o estado do Auto Boss
local autoBossEnabled = false
local autoBossCoroutine

-- Fun칞칚o que cont칠m o script de Auto Boss
local function autoBossScript()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
    local elevatorSpeed = 85 -- Velocidade do elevador
    local combateInterval = 0.5 -- Intervalo entre golpes

    -- Exibir mensagem no in칤cio do script
    local function showMessage(message, duration)
        local msg = Instance.new("Message", game.Workspace)
        msg.Text = message
        wait(duration)
        msg:Destroy()
    end

    -- Mostrar mensagem de aviso
    showMessage("N츾O USE EM SERVER PUBLICO !", 1)

    -- Sistema de elevador
    local function moveWithElevator(targetPosition)
        local currentPosition = humanoidRootPart.Position
        local direction = (targetPosition - currentPosition).Unit
        local distance = (targetPosition - currentPosition).Magnitude

        for step = 0, distance, elevatorSpeed * 0.1 do
            local newPosition = currentPosition + direction * step
            humanoidRootPart.CFrame = CFrame.new(newPosition)
            wait(0.1)
        end

        -- Certifique-se de que o jogador est치 exatamente na posi칞칚o final
        humanoidRootPart.CFrame = CFrame.new(targetPosition)
    end

    -- Fun칞칚o para simular clique no ClickDetector
    local function fireClickDetector(clickDetector)
        if clickDetector then
            fireclickdetector(clickDetector)
            print("ClickDetector acionado com sucesso")
        else
            warn("ClickDetector n칚o encontrado")
        end
    end

    -- Fun칞칚o para equipar combate
    local function equiparCombate()
        if player then
            local backpack = player.Backpack
            local combateItem = backpack:FindFirstChild("Combat")
            if combateItem then
                combateItem.Parent = player.Character
                print("Combate equipado")
            else
                warn("Item 'Combat' n칚o encontrado na mochila.")
            end
        else
            warn("Jogador n칚o encontrado.")
        end
    end

    -- Fun칞칚o para desequipar combate
    local function desequiparCombate()
        if player and player.Character then
            local combateItem = player.Character:FindFirstChild("Combat")
            if combateItem then
                combateItem.Parent = player.Backpack
                print("Combate desequipado")
            end
        end
    end

    -- Fun칞칚o para bater no boss
    local function baterNoBoss()
        local combatEvent = game:GetService("ReplicatedStorage").Events.combat
        if combatEvent then
            combatEvent:FireServer() -- Simula o evento de combate
            print("Bateu no boss")
        else
            warn("Evento de combate n칚o encontrado")
        end
    end

    -- Fun칞칚o para verificar se o boss est치 presente
    local function bossPresente(bossPosition)
        local success, result = pcall(function()
            return game:GetService("Workspace"):FindPartOnRayWithWhitelist(Ray.new(bossPosition + Vector3.new(0, 100, 0), Vector3.new(0, -200, 0)), {game:GetService("Workspace").Npcs.Folder})
        end)
        return success and result and result:IsDescendantOf(game:GetService("Workspace").Npcs.Folder)
    end

    -- Fun칞칚o para grudar atr치s do boss por 3 segundos e bater no boss
    local function grudarAtrasDoBossEBater(boss)
        local bossHRP = boss:WaitForChild("HumanoidRootPart")
        if bossHRP then
            local startTime = tick() -- Tempo atual
            equiparCombate()
            while autoBossEnabled and tick() - startTime < 3 do
                local attachPosition = bossHRP.Position - bossHRP.CFrame.LookVector * 1 -- Posi칞칚o atr치s do boss
                humanoidRootPart.CFrame = CFrame.new(attachPosition)
                baterNoBoss()
                wait(combateInterval) -- Intervalo entre golpes
            end
            desequiparCombate()
        end
    end

    while autoBossEnabled do
        -- Primeira posi칞칚o: Quest do boss de Karate
        local questPositionKarate = game:GetService("Workspace").Interactive.NPCQUESTBOSSKARATE.NpcTalk.Position
        moveWithElevator(questPositionKarate)

        -- Simula o clique no ClickDetector para pegar a miss칚o imediatamente
        local clickDetectorKarate = game:GetService("Workspace").Interactive.NPCQUESTBOSSKARATE.NpcTalk:FindFirstChildOfClass("ClickDetector")
        fireClickDetector(clickDetectorKarate)

        -- Espera 7 segundos na posi칞칚o da miss칚o
        wait(7)

        -- Segunda posi칞칚o: Boss de Karate
        local bossPositionKarate = game:GetService("Workspace").Npcs.Folder.Karate_Boss.PlayerBox.Position
        moveWithElevator(bossPositionKarate)

        -- Espera at칠 que o boss de Karate esteja presente
        while not bossPresente(bossPositionKarate) and autoBossEnabled do
            wait(1)
        end

        -- Grudar atr치s do boss por 3 segundos e bater no boss
        grudarAtrasDoBossEBater(game:GetService("Workspace").Npcs.Folder.Karate_Boss)

        -- Espera 15 segundos ap칩s derrotar o boss antes de continuar para a pr칩xima miss칚o
        wait(12)

        -- Primeira posi칞칚o: Quest do boss de Kung Fu
        local questPositionKungFu = game:GetService("Workspace").Interactive.NPCQUESTBOSSKUNGFU.NpcTalk.Position
        moveWithElevator(questPositionKungFu)

        -- Simula o clique no ClickDetector para pegar a miss칚o imediatamente
        local clickDetectorKungFu = game:GetService("Workspace").Interactive.NPCQUESTBOSSKUNGFU.NpcTalk:FindFirstChildOfClass("ClickDetector")
        fireClickDetector(clickDetectorKungFu)

        -- Espera 7 segundos na posi칞칚o da miss칚o
        wait(7)

        -- Segunda posi칞칚o: Boss de Kung Fu
        local bossPositionKungFu = game:GetService("Workspace").Npcs.Folder.KungFu_Boss.PlayerBox.Position
        moveWithElevator(bossPositionKungFu)

        -- Espera at칠 que o boss de Kung Fu esteja presente
        while not bossPresente(bossPositionKungFu) and autoBossEnabled do
            wait(1)
        end

        -- Grudar atr치s do boss por 3 segundos e bater no boss
        grudarAtrasDoBossEBater(game:GetService("Workspace").Npcs.Folder.KungFu_Boss)

        -- Espera 15 segundos ap칩s derrotar o boss antes de continuar para a pr칩xima miss칚o
        wait(12)
    end
end

-- Adicione um bot칚o toggle na aba "Farms" para o Auto Boss
TabFarms:AddToggle({
    Name = "Auto Farm 2 Boss",
    Default = false,
    Callback = function(value)
        autoBossEnabled = value
        if autoBossEnabled then
            print("Auto Boss ativado")
            autoBossCoroutine = coroutine.create(autoBossScript)
            coroutine.resume(autoBossCoroutine)
        else
            print("Auto Boss desativado")
            if autoBossCoroutine then
                coroutine.close(autoBossCoroutine)
                autoBossCoroutine = nil
            end
        end
    end
})

TabFarms:AddLabel("Esse script Possui um intervalo de 11 segundos para ir pro Boss")
TabFarms:AddLabel("Para garantir que o outro boss tenha nascidoe n칚o bugar")


