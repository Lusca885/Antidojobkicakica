        else
            warn("Item 'Combat' n칚o encontrado na mochila.")
        end
    else
        warn("Jogador n칚o encontrado.")
    end
end

local function desequiparCombate()
    if player and combatItem and player.Character then
        combatItem.Parent = player.Backpack
    end
end

local function cicloEquiparDesequipar()
    while running do
        equiparCombate()
        wait(0.3)
        desequiparCombate()
        wait(0.3)
    end
end

